import math

def is_perfect(n):
    tong = 0
    for i in range(1, int(math.sqrt(n)) + 1):
        if n % i == 0:
            tong += i
            if i != n // i:
                tong += n // i
    # trừ n vì tổng ở trên đã tính cả n
    return (tong - n) == n

n = 1000
for i in range(1, n + 1):
    if is_perfect(i):
        print(i, end=" ")
